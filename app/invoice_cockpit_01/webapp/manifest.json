                    "pattern": ":?query:",
                    "name": "NewList",
                    "target": "NewList"
                },
                {
                    "pattern": "New({key}):?query:",
                    "name": "NewObjectPage",
                    "target": "NewObjectPage"
                },
                {
                    "pattern": "Draft({key}):?query:",
                    "name": "DraftObjectPage",
