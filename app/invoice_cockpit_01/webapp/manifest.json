                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "invoice_cockpit_01-display": {
                    "semanticObject": "invoice_cockpit_01",
